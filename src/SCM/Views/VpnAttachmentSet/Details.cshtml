@model SCM.Models.ViewModels.VpnAttachmentSetViewModel

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div>
    <h4>Attachment Set for VPN '@Model.Vpn.Name'</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Vpn)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Vpn.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.AttachmentSet)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AttachmentSet.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.AttachmentSet.Description)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AttachmentSet.Description)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.AttachmentSet.Tenant)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AttachmentSet.Tenant.Name)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.VpnAttachmentSetID }) |
    <a asp-action="GetAllByVpnID" asp-route-id="@ViewBag.Vpn.VpnID">Back to Attachment Sets</a>
</div>
